{
  "name": "gurugram",
  "version": "1.0.0",
  "private": true,
  "description": "Heartfulness Gurugram",
  "author": "Heartfulness Team",
  "keywords": [
    "heartfulness",
    "gurugram"
  ],
  "scripts": {
    "precommit": "yarn run lint:fix && yarn run depcheck",
    "lint:fix": "eslint --ext .jsx,.js ./ --fix",
    "lint": "eslint --ext .jsx,.js ./ ",
    "build": "gatsby build",
    "develop": "set REACT_APP_ADMIN_BASE_URI=https://dev.heartfuleducation.org/admin-api/api/v1/&&set REACT_APP_ADMIN_PROJECT_ID=127&&set REACT_APP_ADMIN_MENU_ID=51&& gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "depcheck": "depcheck --ignore-patterns=.cache/,public/ --ignores=redux-logger,gatsby-plugin*eslint"
  },
  "dependencies": {
    "@wordpress/shortcode": "^3.9.0",
    "axios": "^0.24.0",
    "bootstrap": "^5.1.3",
    "gatsby": "^4.2.0",
    "gatsby-plugin-google-gtag": "^4.12.1",
    "gatsby-plugin-google-tagmanager": "^4.12.1",
    "gatsby-plugin-image": "^2.0.0",
    "gatsby-plugin-manifest": "^4.0.0",
    "gatsby-plugin-react-helmet": "^5.2.0",
    "gatsby-plugin-sass": "^5.0.0",
    "gatsby-plugin-sharp": "^4.0.1",
    "gatsby-remark-images": "^6.12.1",
    "gatsby-remark-lazy-load": "^1.0.2",
    "gatsby-source-filesystem": "^4.0.0",
    "gatsby-transformer-remark": "^5.12.1",
    "gatsby-transformer-sharp": "^4.0.0",
    "html-react-parser": "^1.4.12",
    "lazysizes": "^5.3.2",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-bootstrap": "^2.4.0",
    "react-datepicker": "^4.4.0",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.19.5",
    "react-multi-carousel": "^2.6.5",
    "react-toastify": "^8.1.0",
    "sass": "^1.43.4",
    "theme-ui": "^0.12.0",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "depcheck": "^1.4.2",
    "prettier": "^2.4.1"
  }
}
